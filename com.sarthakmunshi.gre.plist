<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.username.gre</string>

  <key>Program</key>
  <string>/Users/username/scriptlocation/gre.sh</string>


  <key>StartInterval</key>
  <integer>60</integer>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardErrorPath</key>
  <string>/tmp/com.username.gre.err</string>

  <key>StandardOutPath</key>
  <string>/tmp/com.username.gre.out</string>
</dict>
</plist>